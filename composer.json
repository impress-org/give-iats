{
    "name": "impress-org/give-iats",
    "description": "Give - iATS Payment Solutions - Process online donations via the iATS payment gateway.",
    "homepage": "https://givewp.com/",
    "autoload": {
        "psr-4": {
            "Ravinderkumar\\GiveIats\\": "src/"
        }
    },
    "require": {
        "inacho/php-credit-card-validator": "^1.0",
        "cweagans/composer-patches": "^1.7"
    },
    "config": {
        "platform": {
            "php": "5.6"
        },
        "allow-plugins": {
            "cweagans/composer-patches": true
        }
    },
    "extra": {
        "patches": {
            "inacho/php-credit-card-validator": {
                "PHP 8.1 compatibility issue": "patches/php_8_1_compatibility.patch"
            }
        }
    }
}
